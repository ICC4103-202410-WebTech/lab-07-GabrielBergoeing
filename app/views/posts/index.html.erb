<h1>Hola</h1>
<p><%= @posts.length =%></p>

<table>
	<tr>
	  <td>Title</td>
	  <td>Content</td>
	  <td>Link</td>
	</tr>
	<% @posts.each do |post| %>
	<tr>
	  <td> <%=post.title=%> </td>
	  <td> <%=post.content=%> </td>
	  <td> <%=link_to 'Ver', post_path(post)=%> </td>
	</tr>
	<% end %>
</table>